{
  "name": "@rnwl/service-backend-sharing",
  "version": "0.0.2",
  "description": "Backend service to handle all data and functions relating to item/account sharing",
  "main": "index.ts",
  "scripts": {
    "start": "rm -rf dist && tsc && nodemon -r dotenv/config ./dist/index.js",
    "watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"nodemon -r dotenv/config ./dist/index.js\"",
    "test": "rm -rf dist && npm run lint && tsc && jest --silent",
    "test-watch": "rm -rf dist && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"jest --silent\"",
    "test-dev": "rm -rf dist && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"jest -- people-id-delete.test.js\"",
    "types": "rm -rf dist && tsc",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "check": "rm -rf dist && tsc && eslint . --ext .ts",
    "start-production": "rm -rf node_modules && npm install --production && npm install nodemon && rm -rf dist && tsc --project tsconfig.production.json && nodemon -r dotenv/config ./dist/index.js"
  },
  "keywords": [],
  "author": "Rnwl",
  "license": "ISC",
  "homepage": "https://github.com/Rnwl/service-backend-sharing#readme",
  "dependencies": {
    "@rnwl/package-api-utils": "^3.6.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.7.10",
    "@types/umzug": "^2.3.2",
    "@types/validator": "^13.6.3",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.2",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5",
    "typescript": "^4.4.2",
    "umzug": "^2.3.0"
  },
  "devDependencies": {
    "@rnwl/package-api-dev-utils": "^2.1.0",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cookie": "^0.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "factory.ts": "^0.5.2",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "sqlite3": "npm:@vscode/sqlite3@^5.0.7",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "tsc-watch": "^4.5.0"
  },
  "jest": {
    "rootDir": "dist",
    "setupFiles": [
      "<rootDir>/tests/setTestEnvVars.js"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  }
}
