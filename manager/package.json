{
    "name": "crawl-manager",
    "version": "1.0.0",
    "description": "Backend service to handle all data and functions relating to item/account sharing",
    "main": "index.ts",
    "scripts": {
        "start": "rm -rf dist && tsc && nodemon -r dotenv/config ./dist/manager/index.js",
        "watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"nodemon -r dotenv/config ./dist/manager/index.js\"",
        "test": "rm -rf dist && npm run lint && tsc && jest --silent",
        "test-watch": "rm -rf dist && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"jest --silent\"",
        "test-dev": "rm -rf dist && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"jest -- people-id-delete.test.js\"",
        "types": "rm -rf dist && tsc",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts --fix",
        "check": "rm -rf dist && tsc && eslint . --ext .ts",
        "start-production": "rm -rf node_modules && npm install --production && npm install nodemon && rm -rf dist && tsc --project tsconfig.production.json && nodemon -r dotenv/config ./dist/index.js"
    },
    "keywords": [],
    "author": "Rnwl",
    "license": "ISC",
    "homepage": "https://github.com/Rnwl/service-backend-sharing#readme",
    "dependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.7.10",
        "axios": "^0.21.4",
        "express": "^4.17.1",
        "typescript": "^4.4.2"
    },
    "devDependencies": {
        "@types/cookie": "^0.4.1",
        "@types/jest": "^27.0.1",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.1.0",
        "nodemon": "^2.0.12",
        "prettier": "^2.3.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.5.0",
        "tsc-watch": "^4.5.0"
    },
    "engines": {
        "npm": ">=8.0.0",
        "node": ">=14.0.0"
    },
    "jest": {
        "rootDir": "dist",
        "setupFiles": [
            "<rootDir>/tests/setTestEnvVars.js"
        ],
        "moduleFileExtensions": [
            "js"
        ]
    }
}
